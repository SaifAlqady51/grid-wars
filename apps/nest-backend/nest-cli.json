{
  "monorepo": true,
  "root": "apps/main",
  "sourceRoot": "apps/main/src",
  "compilerOptions": {
    "webpack": false,
    "tsConfigPath": "apps/main/tsconfig.app.json"
  },
  "projects": {
    "account-service": {
      "type": "application",
      "root": "apps/account-service",
      "entryFile": "main",
      "sourceRoot": "apps/account-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/account-service/tsconfig.app.json"
      }
    },
    "aws-s3": {
      "type": "library",
      "root": "libs/aws-s3",
      "entryFile": "index",
      "sourceRoot": "libs/aws-s3/src",
      "compilerOptions": {
        "tsConfigPath": "libs/aws-s3/tsconfig.lib.json"
      }
    },
    "common": {
      "type": "library",
      "root": "libs/common",
      "entryFile": "index",
      "sourceRoot": "libs/common/src",
      "compilerOptions": {
        "tsConfigPath": "libs/common/tsconfig.lib.json"
      }
    },
    "jwt": {
      "type": "library",
      "root": "libs/jwt",
      "entryFile": "index",
      "sourceRoot": "libs/jwt/src",
      "compilerOptions": {
        "tsConfigPath": "libs/jwt/tsconfig.lib.json"
      }
    },
    "main": {
      "type": "application",
      "root": "apps/main",
      "entryFile": "main",
      "sourceRoot": "apps/main/src",
      "compilerOptions": {
        "tsConfigPath": "apps/main/tsconfig.app.json"
      }
    }
  }
}